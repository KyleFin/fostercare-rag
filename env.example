# Foster Care RAG Application Environment Variables
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (required for chat functionality and RAG embedding)
OPENAI_API_KEY=your_openai_api_key_here

# Tavily API Key (required if you want to allow agent to perform web searches for more recent sources)
TAVILY_API_KEY=

# LangChain API Key (required if you want to use LangSmith for tracing and RAG evaluation)
LANGCHAIN_API_KEY=

# Cohere API Key (required if using RAG re-ranking to get more relevant sources)
COHERE_API_KEY=your_cohere_api_key_here

# Optional: Override default ports
# FRONTEND_PORT=3000
# API_PORT=8000
